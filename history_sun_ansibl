---
- hosts: agent
  tasks:
  - name: finding the directory of /root/hello
    stat:
     path: /root/hello
    register: dir
  - debug: msg="{{dir}}"
  - name: create file in /root/hello
    file: path=/root/hello state=touch
    when: dir.stat.exists==true

